commands:
  01_move_vendor_back:
    command: |
      if [ ! -e "/var/www/html/.code-build" ]; then
        cp -r /var/www/vendor_backup /var/www/html/backend/vendor
      fi

  02_move_node_modules_back:
    command: |
      if [ ! -e "/var/www/html/.code-build" ]; then
        cp -r /var/www/node_modules_backup /var/www/html/frontend/node_modules
      fi
